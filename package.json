{
  "name": "@mattgrill/storyblok-11ty",
  "version": "2.3.0",
  "description": "Import Stories and Datasources from Storyblok to 11ty as data objects or static files and it adds custom tags for blocks parsing.",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rspack build --mode production && tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:watch": "rspack build --mode development --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run type-check",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "Storyblok",
    "11ty",
    "eleventy",
    "static-site",
    "cms",
    "typescript"
  ],
  "deprecated": false,
  "author": {
    "name": "Christian Zoppi",
    "email": "me@christianzoppi.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@storyblok/richtext": "^3.8.2",
    "storyblok-js-client": "^7.2.2"
  },
  "devDependencies": {
    "@rspack/cli": "^1.1.8",
    "@rspack/core": "^1.1.8",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattgrill/storyblok-11ty"
  },
  "homepage": "https://github.com/mattgrill/storyblok-11ty#readme",
  "bugs": {
    "url": "https://github.com/mattgrill/storyblok-11ty/issues"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
